{
  "name": "talbinah-portal",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng cache clean && npm install && ng build",
    "start:local": "ng serve --configuration inmemory",
    "prebuild": "ng cache clean && npm install",
    "build:dev": "npm run prebuild && ng build --configuration=inmemory",
    "build:uat": "npm run prebuild && ng build --configuration=uat",
    "build:live": "npm run prebuild && ng build --configuration=production",
    "build:redesign": "npm run prebuild && ng build --configuration=redesign",
    "build:ssr": "ng build && ng run talbinah-portal:server",
    "build:ssr-dev": "npm run prebuild && ng build --configuration=development --aot --progress && ng run talbinah-portal:server:development",
    "build:ssr-redesign": "npm run prebuild && ng build --configuration=redesign --aot --progress && ng run talbinah-portal:server:redesign",
    "build:ssr-uat": "npm run prebuild && ng build --configuration=uat --aot --progress && ng run talbinah-portal:server:production",
    "build:ssr-live": "npm run prebuild && ng build --configuration=production --aot --progress && ng run talbinah-portal:server:production",
    "start:ssr": "node dist/talbinah-portal/server/main.js",
    "serve:ssr:talbinah-portal": "node dist/talbinah-portal/server/server.mjs",
    "upload:s3": "node scripts/deploy/upload-s3.mjs",
    "upload:scp": "node scripts/deploy/upload-scp.mjs",
    "upload:sftp": "node scripts/deploy/upload-sftp.mjs",
    "test": "ng test"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "19.2.10",
    "@angular/cdk": "19.2.10",
    "@angular/common": "19.2.10",
    "@angular/compiler": "19.2.10",
    "@angular/core": "19.2.10",
    "@angular/fire": "^19.2.0",
    "@angular/forms": "19.2.10",
    "@angular/platform-browser": "19.2.10",
    "@angular/platform-browser-dynamic": "19.2.10",
    "@angular/platform-server": "19.2.10",
    "@angular/router": "19.2.10",
    "@angular/ssr": "19.2.10",
    "@ngx-translate/core": "~16.0.4",
    "@ngx-translate/http-loader": "~16.0.1",
    "@primeng/themes": "~19.0.9",
    "agora-rtc-sdk-ng": "^4.23.4",
    "express": "^4.21.2",
    "firebase": "^11.10.0",
    "primeicons": "~7.0.0",
    "primeng": "~19.0.9",
    "rxjs": "~7.8.0",
    "socket.io-client": "^4.8.1",
    "tslib": "~2.3.0",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "19.2.10",
    "@angular/cli": "19.2.10",
    "@angular/compiler-cli": "19.2.10",
    "@types/express": "^4.17.17",
    "@types/jasmine": "^5.1.0",
    "@types/node": "^18.18.0",
    "autoprefixer": "^10.4.21",
    "compressing": "^1.10.0",
    "jasmine-core": "^5.4.0",
    "karma": "^6.4.2",
    "karma-chrome-launcher": "^3.2.0",
    "karma-coverage": "^2.2.0",
    "karma-jasmine": "^5.1.0",
    "karma-jasmine-html-reporter": "^2.1.0",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.17",
    "typescript": "~5.5.2"
  }
}